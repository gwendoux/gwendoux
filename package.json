{
  "name": "gwendoux",
  "version": "2.0.0",
  "description": "my personal website",
  "main": "www/index.html",
  "repository": "git@github.com:gwendoux/gwendoux.git",
  "keywords": [
    "personal",
    "javascript",
    "css",
    "html"
  ],
  "author": "gwendoux",
  "license": "ISC",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "posttest": "npm run lint",
    "test": "",
    "start": "browser-sync start --config bs-config.js",
    "start:watch": "parallelshell 'npm run build:watch' 'npm start'",
    "build:index:css": "lessc www/less/main.less | postcss -u autoprefixer -u pixrem -c postcss.json | cleancss > www/css/main.min.css",
    "build:resume:css": "lessc www/less/resume.less | postcss -u autoprefixer -u pixrem -c postcss.json | cleancss > www/css/resume.min.css",
    "prebuild:css": "npm run clean:css",
    "build:css": "parallelshell 'npm run build:index:css' 'npm run build:resume:css'",
    "prebuild:css:watch": "npm run clean:css",
    "build:css:watch": "watch 'npm run build:css' resource/less/",
    "build": "grunt build",
    "build:watch": "grunt build-watch",
    "clean:js": "rimraf www/js/*.min.*",
    "clean:css": "rimraf www/css/**",
    "clean:dist": "rimraf dist",
    "clean": "parallelshell 'npm run clean:css' 'npm run clean:js'",
    "prepackage": "rimraf dist && npm run build",
    "package": "ncp www/ dist",
    "postpackage": "rimraf dist/less dist/svg/index dist/svg/resume dist/svg/stock dist/js/lib .DS_Store",
    "push:prod": "rsync -avzP --delete-before dist/ gwendoux:/usr/share/nginx/www"
  },
  "devDependencies": {
    "autoprefixer": "6.3.1",
    "browser-sync": "2.10.1",
    "browserify": "13.0.0",
    "clean-css": "3.4.9",
    "convict": "1.0.2",
    "eslint": "1.10.3",
    "grunt": "0.4.5",
    "grunt-appcache": "0.1.8",
    "grunt-browserify": "4.0.1",
    "grunt-chmod": "1.1.1",
    "grunt-concurrent": "2.1.0",
    "grunt-contrib-clean": "0.7.0",
    "grunt-contrib-connect": "0.11.2",
    "grunt-contrib-copy": "0.8.2",
    "grunt-contrib-uglify": "0.11.0",
    "grunt-contrib-watch": "0.6.1",
    "grunt-render-nunjucks": "0.1.3",
    "grunt-rsync": "0.6.2",
    "grunt-svgmin": "3.1.0",
    "grunt-svgstore": "0.5.0",
    "less": "2.5.3",
    "load-grunt-tasks": "3.3.0",
    "parallelshell": "2.0.0",
    "pixrem": "3.0.0",
    "postcss-cli": "2.4.0",
    "rimraf": "2.4.4",
    "time-grunt": "1.2.2",
    "watch": "0.17.1"
  },
  "dependencies": {
    "jquery": "2.1.4",
    "loglevel": "1.4.0",
    "moment": "2.10.6"
  }
}
