{
  "name": "gwendoux",
  "version": "2.1.1",
  "description": "my personal website",
  "main": "www/index.html",
  "repository": "git@github.com:gwendoux/gwendoux.git",
  "keywords": [
    "personal",
    "javascript",
    "css",
    "html"
  ],
  "author": "gwendoux",
  "license": "ISC",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postinstall": "npm prune",
    "start": "browser-sync start --config bs-config.js",
    "start:watch": "parallelshell 'npm run build:watch' 'npm start'",
    "build:index:css": "mkdirp www/css && lessc www/less/main.less | postcss -u autoprefixer -u pixrem -c postcss.json | cleancss > www/css/main.min.css | purifycss www/css/main.min.css www/index.html www/js/lib/behavior.js --output www/css/main.min.css --info",
    "build:resume:css": "mkdirp www/css && lessc www/less/resume.less | postcss -u autoprefixer -u pixrem -c postcss.json | cleancss > www/css/resume.min.css | purifycss www/css/resume.min.css www/resume.html --info",
    "prebuild:css": "npm run clean:css",
    "build:css": "parallelshell 'npm run build:index:css' 'npm run build:resume:css'",
    "prebuild:css:watch": "npm run clean:css",
    "build:css:watch": "watch 'npm run build:css' resource/less/",
    "prebuild:js": "npm run clean:js",
    "prebuild:js:watch": "npm run clean:js",
    "build:js": "browserify www/js/lib/behavior.js | uglifyjs -v --compress --screw-ie8 --stats - > www/js/script.min.js",
    "build:js:watch": "watchify www/js/lib/behavior.js -o www/js/script.min.js -v",
    "prebuild:template": "npm run clean:template",
    "build:template": "grunt renderNunjucks",
    "prebuild:template:watch": "npm run clean:template",
    "build:template:watch": "watch 'npm run build:template' templates/",
    "prebuild:svg": "npm run clean:svg",
    "build:svg": "grunt svgstore",
    "prebuild:svg:watch": "npm run clean:svg",
    "build:svg:watch": "watch 'npm run build:svg' resource/svg/",
    "build": "npm run build:template && npm run build:css && npm run build:js && npm run build:svg",
    "build:watch": "parallelshell 'npm run build:css ''npm run build:js' 'grunt build-watch'",
    "clean:svg": "rimraf www/svg/*",
    "clean:template": "rimraf www/*.html",
    "clean:js": "rimraf www/js/*.min.*",
    "clean:css": "rimraf www/css/**",
    "clean:dist": "rimraf dist",
    "clean": "parallelshell 'npm run clean:css' 'npm run clean:js'",
    "prepackage": "rimraf dist && npm run build",
    "package": "ncp www/ dist",
    "postpackage": "rimraf dist/less dist/svg/index dist/svg/resume dist/svg/stock dist/js/lib .DS_Store",
    "push:prod": "rsync -avzP --delete-before dist/ gwendoux:/usr/share/nginx/www",
    "safefreeze": "rimraf node_modules npm-shrinkwrap.json && npm install && npm prune && npm dedupe && npm lint && npm shrinkwrap",
    "compress:index": "purifycss www/css/main.min.css www/index.html --info"
  },
  "devDependencies": {
    "autoprefixer": "6.3.1",
    "browser-sync": "2.11.1",
    "browserify": "13.0.0",
    "clean-css": "3.4.9",
    "convict": "1.0.2",
    "eslint": "1.10.3",
    "grunt": "0.4.5",
    "grunt-available-tasks": "^0.6.1",
    "grunt-render-nunjucks": "0.1.3",
    "grunt-svgstore": "0.5.0",
    "less": "2.5.3",
    "load-grunt-tasks": "3.4.0",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "parallelshell": "2.0.0",
    "pixrem": "3.0.0",
    "postcss-cli": "2.4.0",
    "purify-css": "1.0.17",
    "rimraf": "2.5.0",
    "uglify-js": "2.6.1",
    "watch": "0.17.1",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "jquery": "2.2.0",
    "loglevel": "1.4.0",
    "moment": "2.11.1"
  }
}
