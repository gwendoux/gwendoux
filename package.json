{
  "name": "gwendoux",
  "version": "2.1.5",
  "description": "my personal website",
  "main": "www/index.html",
  "repository": "git@github.com:gwendoux/gwendoux.git",
  "keywords": [
    "personal",
    "javascript",
    "css",
    "html"
  ],
  "author": "gwendoux",
  "license": "ISC",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postinstall": "npm prune",
    "start": "browser-sync start --config bs-config.js",
    "build:css:index": "mkdirp www/css && lessc www/less/main.less | postcss -u autoprefixer -u pixrem -c postcss.json | cleancss > www/css/main.min.css",
    "postbuild:css:index": "purifycss www/css/main.min.css www/index.html www/js/lib/behavior.js --out www/css/main.min.css --info",
    "build:css:resume": "mkdirp www/css && lessc www/less/resume.less | postcss -u autoprefixer -u pixrem -c postcss.json | cleancss > www/css/resume.min.css",
    "postbuild:css:resume": "purifycss www/css/resume.min.css www/resume.html --out www/css/resume.min.css --info",
    "prebuild:css": "rimraf www/css/**",
    "build:css": "parallelshell 'npm run build:css:index' 'npm run build:css:resume'",
    "prebuild:js": "rimraf www/js/*.min.*",
    "build:js": "browserify www/js/lib/behavior.js | uglifyjs - -v --compress --screw-ie8 > www/js/script.min.js",
    "build:js:watch": "watchify www/js/lib/behavior.js -o www/js/script.min.js -v",
    "prebuild:svg": "rimraf www/svg/*",
    "build:svg:index": "spritesh --input svg/index/ --output www/svg/ss--index-icons.svg --prefix icon-",
    "build:svg:resume": "spritesh --input svg/resume/ --output www/svg/ss--resume-icons.svg --prefix icon-",
    "build": "npm run build:css && npm run build:js && npm run build:svg:index && npm run build:svg:resume",
    "clean": "rimraf  www/svg/* www/*.html www/js/*.min.* www/css/** dist",
    "prepackage": "rimraf dist && npm run build",
    "package": "ncp www/ dist",
    "postpackage": "rimraf dist/less dist/svg/index dist/svg/resume dist/svg/stock dist/js/lib .DS_Store",
    "push:prod": "rsync -avzP --update --delete-before dist/ gwendoux:/usr/share/nginx/www",
    "safefreeze": "rm -rf node_modules npm-shrinkwrap.json && npm install && npm prune && npm dedupe && npm run lint && npm shrinkwrap"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "browser-sync": "2.11.2",
    "browserify": "13.0.0",
    "browserify-shim": "^3.8.12",
    "clean-css": "3.4.11",
    "convict": "1.2.0",
    "eslint": "2.7.0",
    "less": "2.6.1",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "parallelshell": "2.0.0",
    "pixrem": "3.0.0",
    "postcss-cli": "2.5.1",
    "purify-css": "1.1.9",
    "rimraf": "2.5.2",
    "spritesh": "1.0.10",
    "uglify-js": "2.6.2",
    "watchify": "3.7.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:$"
  },
  "dependencies": {
    "loglevel": "1.4.0",
    "moment": "2.12.0",
    "url": "0.11.0"
  }
}
