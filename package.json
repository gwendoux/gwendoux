{
  "name": "gwendoux",
  "version": "2.1.5",
  "description": "my personal website",
  "main": "www/index.html",
  "repository": "git@github.com:gwendoux/gwendoux.git",
  "keywords": [
    "personal",
    "javascript",
    "css",
    "html"
  ],
  "author": "gwendoux",
  "license": "ISC",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postinstall": "npm prune",
    "start": "browser-sync start --config bs-config.js",
    "start:watch": "parallelshell 'npm run build:watch' 'npm start'",
    "build:css:index": "mkdirp www/css && lessc www/less/main.less | postcss -u autoprefixer -u pixrem -c postcss.json | cleancss > www/css/main.min.css",
    "postbuild:css:index": "purifycss www/css/main.min.css www/index.html www/js/lib/behavior.js node_modules/fluidbox/src/js/jquery.fluidbox.js --out www/css/main.min.css --info",
    "build:css:resume": "mkdirp www/css && lessc www/less/resume.less | postcss -u autoprefixer -u pixrem -c postcss.json | cleancss > www/css/resume.min.css",
    "postbuild:css:resume": "purifycss www/css/resume.min.css www/resume.html --out www/css/resume.min.css --info",
    "prebuild:css": "npm run clean:css",
    "build:css": "parallelshell 'npm run build:css:index' 'npm run build:css:resume'",
    "build:css:watch": "watch 'npm run build:css' www/less/",
    "prebuild:svg:index": "rimraf www/svg/dist/ss--index-icons.svg",
    "build:svg:index": "spritesh --input www/svg/index/ --output www/svg/dist/ss--index-icons.svg --prefix icon-",
    "prebuild:js": "npm run clean:js",
    "prebuild:js:watch": "npm run clean:js",
    "build:js": "browserify www/js/lib/behavior.js | uglifyjs - -v --compress --screw-ie8 > www/js/script.min.js",
    "build:js:watch": "watchify www/js/lib/behavior.js -o www/js/script.min.js -v",
    "prebuild:template": "npm run clean:template",
    "build:template": "grunt renderNunjucks",
    "build:template:watch": "watch 'npm run build:template' templates/",
    "prebuild:svg": "npm run clean:svg",
    "build:svg": "grunt svgstore",
    "build:svg:watch": "watch 'npm run build:svg' svg/",
    "build": "npm run build:template && npm run build:css && npm run build:js && npm run build:svg",
    "build:watch": "parallelshell 'npm run build:template:watch' 'npm run build:css:watch' 'npm run build:js:watch' 'npm run build:svg:watch'",
    "clean:svg": "rimraf www/svg/*",
    "clean:template": "rimraf www/*.html",
    "clean:js": "rimraf www/js/*.min.*",
    "clean:css": "rimraf www/css/**",
    "clean:dist": "rimraf dist",
    "clean": "parallelshell 'npm run clean:css' 'npm run clean:js'",
    "prepackage": "rimraf dist && npm run build",
    "package": "ncp www/ dist",
    "postpackage": "rimraf dist/less dist/svg/index dist/svg/resume dist/svg/stock dist/js/lib .DS_Store",
    "push:prod": "rsync -avzP --update --delete-before dist/ gwendoux:/usr/share/nginx/www",
    "safefreeze": "rimraf node_modules npm-shrinkwrap.json && npm install && npm prune && npm dedupe && npm run lint && npm shrinkwrap"
  },
  "devDependencies": {
    "autoprefixer": "6.3.3",
    "browser-sync": "2.11.1",
    "browserify": "13.0.0",
    "browserify-shim": "^3.8.12",
    "clean-css": "3.4.9",
    "convict": "1.1.2",
    "eslint": "2.0.0",
    "grunt": "0.4.5",
    "grunt-available-tasks": "^0.6.1",
    "grunt-render-nunjucks": "0.1.3",
    "grunt-svgstore": "0.5.0",
    "less": "2.6.0",
    "load-grunt-tasks": "3.4.0",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "parallelshell": "2.0.0",
    "pixrem": "3.0.0",
    "postcss-cli": "2.5.1",
    "purify-css": "1.1.6",
    "rimraf": "2.5.2",
    "spritesh": "^1.0.10",
    "uglify-js": "2.6.1",
    "watch": "0.17.1",
    "watchify": "3.7.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:$"
  },
  "dependencies": {
    "loglevel": "1.4.0",
    "moment": "2.11.1"
  }
}
